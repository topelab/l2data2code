# To learn more about .editorconfig see https://aka.ms/editorconfigdocs
root = true

# All files
[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = false
charset = utf-8

# Xml files
[*.xml]
indent_size = 2

[*.csproj]
ident_size = 2

[*.config]
ident_size = 2
