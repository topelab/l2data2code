<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:str="clr-namespace:L2Data2CodeUI.Shared.Localize;assembly=L2Data2CodeUI.Shared"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="L2Data2Code.Main.Vars.VarsWindow"
        WindowStartupLocation="CenterOwner"
        ShowInTaskbar="False"
        Title="VarsWindow">
    <Grid>
        <DataGrid Items="{Binding Variables}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="{x:Static str:Strings.Name}"
                                    Width="50*"
                                    IsReadOnly="True"
                                    Binding="{Binding Path=Name}" />
                <DataGridTextColumn Header="{x:Static str:Strings.Value}"
                                    Width="50*"
                                    Binding="{Binding Path=Value}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
